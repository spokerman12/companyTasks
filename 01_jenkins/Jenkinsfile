pipeline {
    agent any

    stages {
        stage('Example Stage 1') {
            steps {
                parallel(
                        "step 1": { echo "hello" },
                        "step 2": { sleep(time:10,unit:"SECONDS") },
                        "step 3": { echo "world" }
                        "step 3": { echo "how" }
                        "step 3": { echo "is" }
                        "step 3": { echo "life?" }

                )
            }
        }
        stage('Example Stage 2') {
            steps {
                parallel(
                        "step 1": { echo "hello" },
                        "step 2": { echo "world" },
                        "step 3": { echo "world" }
                )
            }
        }
    }
}